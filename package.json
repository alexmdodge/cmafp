{
  "name": "cmafp",
  "version": "0.1.0",
  "description": "Portable CMAF WASM parser",
  "main": "cmafp.js",
  "license": "MIT",
  "config": {
    "cmafp": {
      "libDir": "lib/cmafp",
      "lib": "libcmafp.a",
      "input": "cmafp.cpp",
      "output": "cmafp.js"
    }
  },
  "scripts": {
    "build": "(cd ./scripts && ./build.sh)",
    "build:cpp": "(cd ./scripts && ./build-cpp.sh)",
    "test": "npm run build && node ./test/index.js",
    "test:cpp": "npm run build:cpp && (cd ./bin && ./CmafParser ../test/sample/video/avc1/init.mp4)"
  },
  "dependencies": {
    "chalk": "^4.1.1"
  }
}
